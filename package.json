{
  "name": "onur.dev",
  "description": "My personal website.",
  "version": "1.0.0",
  "author": {
    "name": "Onur Şuyalçınkaya",
    "url": "https://onur.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suyalcinkaya/onur.dev.git"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "yarn clear-cache && next dev",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier --write \"**/*.{html,js,json,md,mdx,mjs}\"",
    "svgr-all": "svgr ./src/icons -d ./src/components/icons --svg-props 'className=' --svgo-config .svgorc.json",
    "svgr-new": "svgr ./src/icons -d ./src/components/icons --ignore-existing --svg-props 'className=' --svgo-config .svgorc.json",
    "clear-cache": "rm -rf .next",
    "find:unused": "next-unused"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^10.2.3",
    "@tailwindcss/line-clamp": "^0.2.0",
    "autoprefixer": "^10.2.6",
    "copy-to-clipboard": "^3.3.1",
    "framer-motion": "^4.1.17",
    "gray-matter": "^4.0.2",
    "mdx-prism": "^0.3.1",
    "next": "^10.2.3",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^3.0.2",
    "next-pwa": "^5.2.21",
    "next-seo": "^4.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reading-time": "^1.2.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-code-titles": "^0.1.1",
    "remark-slug": "^6.0.0",
    "tinytime": "^0.2.6"
  },
  "devDependencies": {
    "@svgr/cli": "^5.4.0",
    "@tailwindcss/jit": "^0.1.18",
    "next-unused": "^0.0.3",
    "postcss": "^8.3.0",
    "rehype": "^11.0.0",
    "tailwindcss": "^2.1.4"
  },
  "next-unused": {
    "debug": true,
    "entrypoints": [
      "src"
    ]
  }
}
